{
  "name": "airbnb-alerts",
  "version": "1.0.0",
  "description": "Airbnb listing alert service",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "worker": "node src/workers/scraper-worker.js",
    "migrate": "node src/db/migrate.js",
    "migrate:to-supabase": "node src/db/migrate-to-supabase.js",
    "migrate:to-supabase:verify": "node src/db/migrate-to-supabase.js --skip-verification=false",
    "migrate:specific": "node src/db/migrate-to-supabase.js",
    "setup:replication": "node src/db/setup-replication.js",
    "db:health": "node -e \"import('./src/db/health-check.js').then(m => { const hc = m.healthCheck; hc.check().then(s => console.log(hc.formatStatus(s))); })\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "airbnb",
    "alerts",
    "scraper"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.0",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.19.0",
    "express-validator": "^7.3.1",
    "helmet": "^7.1.0",
    "ical.js": "^1.5.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.9",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.3",
    "stripe": "^14.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.3"
  }
}
